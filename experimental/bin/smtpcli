#!/usr/bin/env python2.4
# vim:ts=4:sw=4
# $Id: smtpcli,v 1.1 2005/07/25 21:25:16 kdart Exp $

"""
SMTP test client. Allows ad-hoc interaction with an SMTP server.

"""


import sys
import inet.smtpCLI

def main(argv):
    try:
        inet.smtpCLI.smtpcli(argv)
    except KeyboardInterrupt:
        return

main(sys.argv)
