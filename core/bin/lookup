"""
Looks up a word in the dict.org server.

"""

import sys

from pycopia.inet import DICT

def main(argv):
    ds = DICT.DictConnection("dict.org")  # "127.0.0.1") 
    for word in argv[1:]:
        wdefs = ds.get_definition(word)
        for wdef in wdefs:
            print "\n".join(wdef)
            print


main(sys.argv)
